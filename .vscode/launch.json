    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ModUpdater/bin/Debug/netcoreapp3.1/ModUpdater.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ModUpdater",
            "console": "internalConsole",
            "stopAtEntry": false,
            "logging": {
                "moduleLoad": false,
             }
        },
        {
            "name": ".NET Core Wandering Trader Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildTrades",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/WanderingTradesUpdater/bin/Debug/netcoreapp3.1/WanderingTradesUpdater.dll",
            "args": [],
            "cwd": "${workspaceFolder}/WanderingTradesUpdater",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false,
            "logging": {
                "moduleLoad": false,
             }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
    ]
}